    "def sum_returns(returns, mode):\n",
    "    if mode == 'risky':\n",
    "        summed_returns = returns[risky].sum(axis=1)\n",
    "    else:\n",
    "        summed_returns = returns['aud_jpy'] + returns['aud_usd'] + (-1 * returns['xau_aud']) + (-1 * returns['eur_aud'])\n",
    "    return summed_returns"
   ]
  },
