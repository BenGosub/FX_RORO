   "source": [
    "def get_cycle_period(peak_index, df, column, threshold):\n",
    "    peak_dt = df[peak_index : peak_index + 1].index[0]\n",
    "    thr = threshold / 3\n",
    "    begin_cycle = df.loc[df[column] <= threshold][ : str(peak_dt)].index.max()\n",
    "    end_cycle = df.loc[df[column] <= threshold][str(peak_dt) :].index.min()\n",
    "    return begin_cycle, end_cycle"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 73,
