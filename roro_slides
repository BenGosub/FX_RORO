    "def calc_peak_indices(y, mode, thres=0.7, min_dist=20):\n",
    "    if mode == 'non_risky':\n",
    "        y = -1 * y\n",
    "    indexes = peakutils.indexes(y, thres=thres, min_dist=min_dist)\n",
    "    return indexes"
   ]
  },
