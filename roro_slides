   "source": [
    "cor_level = correlations_level(returns_df, non_risky, mode='non_risky')\n",
    "condition = cor_level > minimum_correlations_level and \\\n",
    "check_correlations('non_risky', returns_df)\n",
    "\"Conditions hold %s, Correlations level is: %s and Correlations have the proper signs\" % (condition, cor_level)"
   ]
