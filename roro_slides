    "def check_risky_correlation(cycle):\n",
    "    if (cycle.corr()['aud_jpy']['aud_usd'] > 0 and\n",
    "        cycle.corr()['aud_jpy']['xau_aud'] < 0 and\n",
    "        cycle.corr()['aud_jpy']['eur_aud'] < 0):\n",
    "        return True\n",
    "    else:\n",
    "        return False"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 81,
   "metadata": {
    "collapsed": true,
    "slideshow": {
     "slide_type": "skip"
    }
   },
   "outputs": [],
   "source": [
    "def check_non_risky_correlation(cycle):\n",
    "    if (cycle.corr()['usd_jpy']['eur_jpy'] > 0 and\n",
    "        cycle.corr()['usd_jpy']['gbp_jpy'] > 0 and\n",
    "        cycle.corr()['usd_jpy']['aud_jpy'] > 0):\n",
    "        return True\n",
    "    else:\n",
    "        return False"
   ]
  },
