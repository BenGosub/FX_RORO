    "def correlations_level(df, cols, mode):\n",
    "    if mode == 'risky':\n",
    "        correlations_level = sum([df.corr()['aud_jpy']['aud_usd'],\n",
    "        -1 * (df.corr()['aud_jpy']['xau_aud']),\n",
    "        -1 * (df.corr()['aud_jpy']['eur_aud'])]) / 3\n",
    "    else:\n",
    "        correlations = np.unique(returns[non_risky].corr().values)\n",
    "        correlations_level = sum([abs(x) for x in correlations if x != 1]) / len(correlations)\n",
    "    return correlations_level"
   ]
  },
  {
