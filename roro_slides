   "source": [
    "def get_end_date(start, n_periods=18, granularity=0.25):\n",
    "    \"\"\"\n",
    "        Granulairty is in terms of hours, \n",
    "        15min granularity = 0.25,\n",
    "        1 day granularity = 24\n",
    "        4h = 4\n",
    "        1 h = 1\n",
    "        \n",
    "        Start can be a time or a date string.\n",
    "        If time example format is: '2016-06-01 00:00:00'\n",
    "        If date example format is: '2016-06-01'\n",
    "    \"\"\"\n",
    "    if len(start) == 10:\n",
    "        parsed_datetime = datetime.datetime.strptime(start, '%Y-%m-%d')\n",
    "    else:\n",
    "        parsed_datetime = datetime.datetime.strptime(start, '%Y-%m-%dT%H:%M:%S.%f')\n",
    "    end = parsed_datetime + datetime.timedelta(hours=(n_periods * granularity))\n",
    "    return end"
   ]
  },
